@model List<ResultAboutDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}


<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık1</th>
        <th>Başlık2</th>
        <th>Başlık3</th>
        <th>Açıklama</th>
        <th>Resim</th>
        <th>Güncelle</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th> @item.AboutID</th>
            <td> @item.TitleOne</td>
            <td> @item.TitleTwo</td>
            <td> @item.TitleThree</td>
            <td> @item.Description</td>
            <td> @item.Image</td>
            <td> <a href="/AdminAbout/UpdateAbout/@item.AboutID" class="btn mb-1 btn-rounded btn-outline-success">Güncelle</a></td>
        </tr>

    }
</table>