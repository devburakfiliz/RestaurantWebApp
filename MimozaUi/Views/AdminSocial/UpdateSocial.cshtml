@model UpdateSocialDto
@{
    ViewData["Title"] = "UpdateSocial";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}



<form method="post">
    <input type="hidden" asp-for="SocialID" />
    <label>Başlık</label>
    <input type="text" asp-for="Name" class="form-control" />
    <label>Link</label>
    <input type="text" asp-for="Link" class="form-control" />
    <label>Icon</label>
    <input type="text" asp-for="Icon" class="form-control" />


    <br />
    <button class="btn btn-outline-success">Güncelle</button>
</form>
