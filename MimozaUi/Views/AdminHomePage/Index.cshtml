@model List<ResultHomePageDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}


<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık</th>
        <th>Resim</th>
        <th>Açıklama</th>


        <th>Güncelle</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th> @item.HomePageID</th>
            <td> @item.HomePagetTitle</td>
            <td> @item.HomePageImage</td>
            <td> @item.Description</td>


            <td> <a href="/AdminHomePage/UpdateHomePage/@item.HomePageID" class="btn mb-1 btn-rounded btn-outline-success">Güncelle</a></td>
        </tr>

    }
</table>
