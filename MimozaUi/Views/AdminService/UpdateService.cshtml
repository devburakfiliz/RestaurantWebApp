@model UpdateServiceDto
@{
    ViewData["Title"] = "UpdateService";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <input type="hidden" asp-for="ServiceID" />
    <label>Hizmet Adı</label>
    <input type="text" asp-for="ServiceTitle" class="form-control" />
    <label>Hizmet icon</label>
    <input type="text" asp-for="ServiceIcon" class="form-control" />
    <label>Açıklama</label>
    <input type="text" asp-for="Description" class="form-control" />


    <br />
    <button class="btn btn-outline-success">Hizmeti Güncelle</button>
</form>
