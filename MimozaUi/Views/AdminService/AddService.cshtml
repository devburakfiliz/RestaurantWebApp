@model CreateServiceDto
@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}


<form method="post">

    <label>Hizmet Adı</label>
    <input type="text" asp-for="ServiceTitle" class="form-control" />
    <label>Hizmet icon</label>
    <input type="text" asp-for="ServiceIcon" class="form-control" />
    <label>Resim</label>
    <input type="text" asp-for="Description" class="form-control" />



    <br />
    <button class="btn btn-outline-success">Hizmeti Ekle</button>
</form>
